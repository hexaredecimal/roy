
#[extern("__rml_toString"), export]
let from (value: 'a) : String

#[extern("__rml_string_size"), export]
let size (value: 'a) : String

#[extern("__rml_string_fmt"), export]
let format (fmt: String) (args: [Any]) : Unit

#[extern("__rml_sys_string_concat"), export]
let (++) (a: String) (b: String): String

#[extern("__rml_string_split"), export]
let splitBy (delim: String) (value: String)  : [String]

#[export]
let toList (value: String) = 
  splitBy "" value

